{
  "manifest_version": 3,
  "name": "CliffsNotes Paywall Remover",
  "version": "1",
  "description": "Removes paywall from CliffsNotes when clicked.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://www.cliffsnotes.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Click to remove paywall."
  }
}
